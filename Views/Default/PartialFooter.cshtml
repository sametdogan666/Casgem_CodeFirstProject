<section class="w3l-footer-29-main">
    <div class="footer-29 py-5 ">
        <div class="container">
            <div class="grid-col-4 footer-top-29">
                <div class="footer-list-29 footer-1">
                    <h2 class="footer-title-29">Bizimle İletişime Geçin</h2>
                    @foreach(var item in ViewBag.comContact){
                        <ul>
                            <li>
                                <p>
                                    <span class="fa fa-map-marker">@item.Location</span>
                                </p>
                            </li>
                            <li><a href="/Default/Index"><span class="fa fa-phone"></span> @item.PhoneNumber</a></li>
                            <li><a href="mailto:@item.Email" class="mail"><span class="fa fa-envelope-open-o"></span> @item.Email</a></li>
                        </ul>
                    }
                    <div class="main-social-footer-29">
                        <a href="#twitter" class="github"><span class="fa fa-github"></span></a>
                        <a href="#instagram" class="instagram"><span class="fa fa-instagram"></span></a>
                        <a href="#linkedin" class="linkedin"><span class="fa fa-linkedin"></span></a>
                    </div>
                </div>
                <div class="footer-list-29 footer-2">
                    <ul>
                        <h6 class="footer-title-29">Kategori</h6>
                        <li>Etkinlik Seyahati</li>
                        <li>Tatil Gezisi</li>
                        <li>Grup Turları</li>
                        <li>Paket Tatiller</li>
                        <li>Boşluk Yılı</li>
                    </ul>
                </div>
                <div class="footer-list-29 footer-3">
                    <div class="properties">
                        <h6 class="footer-title-29">Galeri</h6>
                        <div class="images-col">
                            <img src="~/Templates/go-trip-starter/assets/images/blog1.jpg" class="img-responsive" alt="">
                            <img src="~/Templates/go-trip-starter/assets/images/blog2.jpg" class="img-responsive" alt="">
                            <img src="~/Templates/go-trip-starter/assets/images/blog3.jpg" class="img-responsive" alt="">
                            <img src="~/Templates/go-trip-starter/assets/images/blog4.jpg" class="img-responsive" alt="">
                            <img src="~/Templates/go-trip-starter/assets/images/blog5.jpg" class="img-responsive" alt="">
                            <img src="~/Templates/go-trip-starter/assets/images/blog6.png" class="img-responsive" alt="">
                        </div>
                    </div>
                </div>
                <div class="footer-list-29 footer-4">
                    <ul>
                        <h6 class="footer-title-29">Seyahat & Gezi</h6>
                        <li>Bahamas</li>
                        <li>Cambodia</li>
                        <li>Dominica</li>
                        <li>Egypt</li>
                        <li>Germany</li>
                    </ul>
                </div>
            </div>
            <div class=" bottom-copies text-center">
                <p class="copy-footer-29">© 2020 Go Trip. All rights reserved | Designed by <a href="//w3layouts.com">W3layouts</a></p>
            </div>
        </div>
    </div>
</section>
